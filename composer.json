{
    "name": "fcpauldiaz/smart-time",
    "description":"ERP para administrar el control de horas de una empresa",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "require": {
        "php": ">=5.5.0",
        "symfony/symfony": "2.8.*",
        "doctrine/orm": "^2.4.8",
        "doctrine/doctrine-bundle": "~1.4",
        "symfony/assetic-bundle": "~2.3",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~4.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "incenteev/composer-parameter-handler": "~2.0",
        "friendsofsymfony/user-bundle": "~2.0@dev",
        "fcpauldiaz/multi-user-bundle": "3.0.*@dev",
        "javiereguiluz/easyadmin-bundle": "^1.9",
        "braincrafted/bootstrap-bundle": "^2.2",
        "twbs/bootstrap": "3.*",
        "oyejorge/less.php": "~1.5",
        "stephanecollot/datetimepicker-bundle": "^1.0",
        "stof/doctrine-extensions-bundle": "^1.2",
        "ibrows/loggable-bundle": "^2.2",
        "leafo/scssphp": "^0.6.3",
        "patchwork/jsqueeze": "~1.0",
        "cspoo/swiftmailer-mailgun-bundle": "^0.3.1",
        "liuggio/excelbundle": "^2.0",
        "aws/aws-sdk-php": "~3.0",
        "knplabs/knp-gaufrette-bundle": "^0.3.0",
        "vich/uploader-bundle": "^1.3.1",
        "zendframework/zend-code": "3.0.1",
        "jms/serializer": "^1.1",
        "jms/serializer-bundle": "^1.1",
        "php-http/guzzle6-adapter": "^1.1",
        "ocramius/proxy-manager": "^2.0.3",
        "hackzilla/ticket-bundle": "^2.0",
        "knplabs/knp-paginator-bundle": "^2.5",
        "emagister/memcached-bundle": "^0.1.4",
        "ext-memcached": "^3.0@beta"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8",
        "phpunit/dbunit": ">=1.2",
        "sensio/generator-bundle": "~2.3",
        "symfony/phpunit-bridge": "~2.7",
        "hautelook/alice-bundle": "^1.2",
        "doctrine/data-fixtures": "^1.1"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
