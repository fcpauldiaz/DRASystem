{% extends '::base.html.twig' %}

{% block body -%}
    <br>
    <h1>RegistroHorasPresupuesto list</h1>

    <table class="table table-striped table-hover">
        <thead>
            <tr>
                <th>Id</th>
                <th>Horaspresupuestadas</th>
                <th>Fechacreacion</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('horaspresupuesto_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.horasPresupuestadas }}</td>
                <td>{% if entity.fechaCreacion %}{{ entity.fechaCreacion|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>
                <ul>
                    <li>
                        <a class="btn btn-default" href="{{ path('horaspresupuesto_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a class="btn btn-default" href="{{ path('horaspresupuesto_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a class="btn btn-default btn-block" href="{{ path('horaspresupuesto_new') }}">
                Crear nuevo registro
            </a>
        </li>
    </ul>
    {% endblock %}
