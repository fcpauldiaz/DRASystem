
<h1>Puesto</h1>
<table class="table table-striped table-hover">
    <tbody>
        <tr>
            <th>Tipo</th>
            <td>{{ entity.tipoPuesto }}</td>
        </tr>
        <tr>
            <th>Departamento Puesto</th>
            <td>{{ entity.departamento }}</td>
        </tr>
        <tr>
            <th>Fecha del puesto</th>
            <td>{{ entity.date|date('d-m-Y') }}</td>
        </tr>
    </tbody>
</table>
<ul class="record_actions">
    <li>
        <a class="btn btn-default" href="{{ path('puesto_edit', { 'id': entity.id }) }}">
            Editar
        </a>
    </li>
    <br>
</ul>
    
