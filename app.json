{
  "name": "DRA Custom Heroku JSON",
  "description": "Template for deploying branches to Heroku",
  "repository": "https://github.com/fcpauldiaz/DRASystem",
  "success_url": "/",
  "env": {
    "S3_BUCKET_NAME": "",
    "CLEARDB_DATABASE_URL": "",
    "MYSQL_HOST": "",
    "MYSQL_NAME": "",
    "MYSQL_PASSWORD": "",
    "EMAIL_PASSWORD": "",
    "EMAIL_TRANSPORT": "",
    "SYMFONY_ENV": "",
    "TEMPORIZE_TOKEN": "",
    "API_USER": "",
    "API_PASSWORD": ""

  },
  "image": "heroku/php",
  "addons": [
    "autobus",
    "logentries",
    "temporizescheduler"
  ],
  "buildpacks": [
    {
      "url": "https://github.com/heroku/heroku-buildpack-php"
    }
  ]
}